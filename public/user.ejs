<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>User Information</title>
    <style>
        td {
            padding: 7px;
        }
    </style>
</head>
<body>
    <table border="1" cellspacing="0" cellpadding="0" align="center">
        <tr>
            <td>Username</td>
            <td>Email</td>
            <td>Password</td>
        </tr>
        <% for(let i=0;i < users.length;i++){ %>
        <tr>
            <td><%= users[i].username %></td>
            <td><%= users[i].email %></td>
            <td><%= users[i].password %></td>
        </tr>
        <% } %>
    </table>
</body>
</html>
