<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <% include ./partial/menu.ejs %>
    <div class="table">
      <table border="1px">
        <tr>
          <th>ID</th>
          <th>Name</th>
          <th>Province</th>
          <th>Options</th>
        </tr>
        <%cities.forEach(city=>{%>
          <tr>
            <td><%=city.id%></td>
            <td><%=city.name%></td>
            <td><%=city.province%></td>
            <td>
              <a href="/cities/edit/<%=city.id%>">edit</a>
              <a href="/cities/delete/<%=city.id%>">delete</a>
            </td>
          </tr>
        <%})%>
      </table>
    </div>
    <a href="/cities/add">add city</a>
  </body>
</html>
