    <title></title>
  </head>
  <body>
    <table>
      <tr>
        <th>id</th>
        <th>nama</th>
        <th>province</th>
      </tr>
      <%for(var i = 0; i < data.length; i++) {%>
      <tr>
        <td><%=data[i].id%></td>
        <td><%=data[i].name%></td>
