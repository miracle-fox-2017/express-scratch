<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <%include ./partials/head%>
    <title>Edit Cities</title>
  </head>
  <body>
      <%include ./partials/menu%>
      <div class="uk-section">
          <div class="uk-container">
              <form action="/cities/edit/<%=id%>" method="post">
                    <div class="uk-margin">
                        <input class="uk-input uk-form-width-medium" value="<%=name%>" name="name" type="text">
                    </div>
                    <div class="uk-margin">
                        <input class="uk-input uk-form-width-medium" value="<%=province%>" name="province" type="text">
                    </div>
                    <p uk-margin>
                        <button class="uk-button uk-button-default uk-button-small">Edit</button>
                    </p>
              </form>
          </div>
      </div>
  </body>
</html>
