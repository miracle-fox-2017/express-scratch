<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <% include ./partial/menu.ejs %>
    <div class="table">
      <table border="1px">
        <tr>
          <th>ID</th>
          <th>Username</th>
          <th>Password</th>
          <th>Email</th>
          <th>Options</th>
        </tr>
        <%users.forEach(user=>{%>
          <tr>
            <td><%=user.id%></td>
            <td><%=user.username%></td>
            <td><%=user.password%></td>
            <td><%=user.email%></td>
            <td>
              <a href="/users/edit/<%=user.id%>">edit</a>
              <a href="/users/delete/<%=user.id%>">delete</a>
            </td>
          </tr>
        <%})%>
      </table>
    </div>
    <a href="/users/add">add user</a>
  </body>
</html>
