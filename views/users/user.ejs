<%- include('../partials/head')%>
<%- include('../partials/menu')%>

<h2>Table Users</h2>
<table class="table table-striped">
	 <thead>
      <tr>
        <th>Username</th>
        <th>Password</th>
        <th>Email</th>
        <th>Options</th>
      </tr>
    </thead>
    <tbody>
    	<% users.forEach(user=>{  %>
      <tr>
        <td><%=user.username%></td>
        <td><%=user.password%></td>
        <td><%=user.email%></td>
        <td><a href="/users/edit/<%=user.id%>" class="btn btn-primary">Edit</a>  
        <a href="/users/delete/<%=user.id%>" class="btn btn-primary">Delete</a>
        <a href="/users/add" class="btn btn-primary">Add</a>
        </td>
      </tr>
      <%})%>
    </tbody>
  </table>

	

<%- include('../partials/footer',{})%>
